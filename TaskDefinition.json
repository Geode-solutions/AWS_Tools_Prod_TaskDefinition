{
    "containerDefinitions": [
        {
            "name": "ToolsContainer",
            "image": "ghcr.io/geode-solutions/geode-backend:develop",
            "cpu": 0,
            "memoryReservation": 1000,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/TD_Tools_Dev",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "TD_Tools_Dev",
    "taskRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::622060531233:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}